@using Signum.Entities.Mailing

@using (var sc = Html.TypeContext<EmailConfigurationDN>()) 
{
	@Html.ValueLine(sc, ca => ca.ReciveEmails)
	@Html.ValueLine(sc, ca => ca.SendEmails)
    @Html.ValueLine(sc, ca => ca.OverrideEmailAddress)
    @Html.EntityCombo(sc, ca => ca.DefaultCulture)
    @Html.ValueLine(sc, ca => ca.UrlLeft)
    @Html.ValueLine(sc, ca => ca.CreationDateHoursLimitToSendEmails)
    @Html.ValueLine(sc, ca => ca.ChunkSizeToProcessEmails)
    @Html.ValueLine(sc, ca => ca.MaxEmailSendRetries)
    @Html.ValueLine(sc, ca => ca.AsyncSenderPeriodMilliseconds)
}
